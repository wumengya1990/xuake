blockquote,body,button,dd,dl,dt,fieldset,form,h1,h2,h3,h4,h5,h6,hr,input,legend,li,ol,p,pre,td,textarea,th,ul {
	margin: 0;
	padding: 0
}
body,html{
	height:100%;
	overflow:hidden;
	color:#666666
}
body,button,input,select,textarea {
	font:14px/1.5 'Microsoft YaHei',tahoma,arial,SimSun,sans-serif;
}

button,input,optgroup,option,select,textarea{
	font-family:inherit;
	font-size:inherit;
	font-style:inherit;
	font-weight:inherit;
	outline:0;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}

h1,h2,h3,h4,h5,h6 {
	font-size: 100%
}

address,cite,dfn,em,var,i{
	font-style: normal
}

code,kbd,pre,samp {
	font-family: courier new,courier,monospace
}

small {
	font-size: 12px
}

ol,ul {
	list-style: none;
}

a {
	color: #333;
	text-decoration:none;
}

a:hover {
	color: #888
}

a:active,a:hover {
	outline:0
}

a cite {
	font-style: normal;
	*cursor: pointer
}

sup {
	vertical-align: text-top
}

sub {
	vertical-align: text-bottom
}

legend {
	color: #000
}

fieldset,img {
	border: 0
}

button,input,select,textarea {
	font-size: 100%
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

button {
	border-radius: 0;
}

/*滚动条*/
::-webkit-scrollbar{width: 9px;height: 9px;background:#F3F3F3;}
::-webkit-scrollbar-thumb{border:1px solid #ffffff;border-radius: 6px;background:#c9c9c9;}
::-webkit-scrollbar-thumb:hover{background: #b5b5b5;}

/*清除浮动*/
.avalon-merlin-clr{clear:both;}
.avalon-merlin-clra{clear:both;font-size:0px;margin:0px;padding:0px;height:0px;line-height:0px;}

/*浮动*/
.avalon-merlin-lfloat{float:left;}
.avalon-merlin-rfloat{float:right;}

/*盒子*/
.avalon-merlin-box{display:flex;display:-webkit-box;display:-moz-box;display:-webkit-flex;display:-moz-flex;display:-ms-flexbox;}
.avalon-merlin-flex1{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1;}
.avalon-merlin-flex2{-moz-box-flex:2;-webkit-box-flex:2;box-flex:2;}
.avalon-merlin-flex3{-moz-box-flex:3;-webkit-box-flex:3;box-flex:3;}
.avalon-merlin-flex4{-moz-box-flex:4;-webkit-box-flex:4;box-flex:4;}
.avalon-merlin-flex5{-moz-box-flex:5;-webkit-box-flex:5;box-flex:5;}

/*框类型*/
.avalon-merlin-hide{display:none;}
.avalon-merlin-show{display:block;}
.avalon-merlin-line-block{
	display:inline-block;
	*display:inline-block;
	*zoom:1;
}
.avalon-merlin-line{display:inline;}

/*文本对齐*/
.avalon-merlin-tac{text-align:center;}
.avalon-merlin-tal{text-align:left;}
.avalon-merlin-tar{text-align:right;}

/*文本加粗*/
.avalon-merlin-bold{font-weight:bold;}
.avalon-merlin-normal{font-weight:normal;}

/*缩进*/
.avalon-merlin-indent{text-indent:2em;}

/*自动换行*/
.avalon-merlin-wordwrap{
	table-layout:fixed;
	word-break:break-all;
	overflow:hidden;
	word-wrap:break-word;
}

/*省略号*/
.avalon-merlin-ellipsis{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}

/*列宽由表格宽度和列宽度设定*/
.avalon-merlin-layout-fixed{table-layout:fixed;}

/*滚动条*/
.avalon-merlin-h-scroll{overflow-x:auto;overflow-y:hidden;}	
.avalon-merlin-v-scroll{overflow-x:hidden;overflow-y:auto;}
.avalon-merlin-hidden{overflow:hidden;}
.avalon-merlin-a-scroll{overflow-x:auto;overflow-y:auto;}

/*定位*/
.avalon-merlin-relative{position:relative;}
.avalon-merlin-absolute{position:absolute;}
.avalon-merlin-fixed{position:fixed;}

/*指针*/
.avalon-merlin-cur-default{cursor:default;}
.avalon-merlin-cur-pointer{cursor:pointer;}

.avalon-merlin-border-box{box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
.avalon-merlin-overline{text-decoration:overline;}
.avalon-merlin-through{text-decoration:line-through;}
.avalon-merlin-underline{text-decoration:underline;}

/*效果*/
.avalon-merlin-ease-in-out{
	-webkit-transition:color .3s ease-in-out;
	-moz-transition:color .3s ease-in-out;
	transition:color .3s ease-in-out;
	-webkit-transition:background-color .3s ease-in-out;
	-moz-transition:background-color .3s ease-in-out;
	transition:background-color .3s ease-in-out;
}

/* 圆型和圆角 */
.avalon-merlin-circle{
	-moz-border-radius:50%;
	-webkit-border-radius:50%;
	border-radius:50%;
}

/*字体颜色*/
.avalon-merlin-fc-disabled{color:#999;}
.avalon-merlin-fc-add{color:#090}
.avalon-merlin-fc-del{color:#ff0000;}
.avalon-merlin-fc-wrong{color:#cc0000;}
.avalon-merlin-fc-important{color:#c60;}
.avalon-merlin-fc-warning{color:#f40;}
.avalon-merlin-fc-integral{color:#FF7F00}
.avalon-merlin-fc-link{color:#0084b7}
.avalon-merlin-fc-txt1{color:#333;}
.avalon-merlin-fc-txt2{color:#666;}
.avalon-merlin-fc-txt3{color:#999;}
.avalon-merlin-fc-txt4{color:#AAAAAA;}
.avalon-merlin-fc-txt5{color:#CCCCCC}

.avalon-merlin-badge{
	background-color:#ff3636;
	color:#FFFFFF;
	position:absolute;
	height:7px;
	width:7px;
	top:11px;
	right:11px;
	text-align:center;
}

/*宽度*/
.avalon-merlin-w{
	width:150px;
}
.avalon-merlin-w-big{
	width:260px;
}
.avalon-merlin-w-small{
	width:200px;
}
.avalon-merlin-w-mini{
	width:100px;
}
.avalon-merlin-wp-10{
	width:10%;
}
.avalon-merlin-wp-20{
	width:20%;
}
.avalon-merlin-wp-30{
	width:30%;
}
.avalon-merlin-wp-40{
	width:40%;
}
.avalon-merlin-wp-50{
	width:50%;
}
.avalon-merlin-wp-60{
	width:60%;
}
.avalon-merlin-wp-70{
	width:70%;
}
.avalon-merlin-wp-80{
	width:80%;
}
.avalon-merlin-wp-90{
	width:90%;
}
.avalon-merlin-wp-94{
	width:94%;
}
.avalon-merlin-w-all{
	width:100%;
}

/*高度*/
.avalon-merlin-h{
	height:32px;
	font-size:14px;
}
.avalon-merlin-h-big{
	height:46px;
	font-size:16px;
}
.avalon-merlin-h-small{
	height:38px;
	font-size:14px;
}
.avalon-merlin-h-mini{
	height:26px;
	font-size:12px;
}

/*外边距*/
.avalon-merlin-mt{margin-top:10px}
.avalon-merlin-mt-mini{margin-top:5px}

/*内边距*/
.avalon-merlin-pa{padding:10px}
.avalon-merlin-pa-small{padding:15px}

/*表单*/
.avalon-merlin-form li{clear:both;padding:10px 0px;}
.avalon-merlin-form li .form-left{display:block;float:left;line-height:32px;}
.avalon-merlin-form li .form-right{display:block;overflow:hidden;}

/*上传*/
.avalon-merlin-upload{width:60px;height:32px;line-height:32px;text-align:center;position:relative;}
.avalon-merlin-upload div{position:absolute;top:0px;bottom:0px;left:0px;right:0px;opacity:0;filter:alpha(opacity=0);}
.avalon-merlin-upload .webuploader-pick{padding:0px;}
.avalon-merlin-upload .webuploader-container div{width:100%;height:100%;}

/*左侧菜单*/
.avalon-merlin-menu{
	width:100%;
}
.avalon-merlin-menu .icons{
	display:block;
	float:left;
	width:24px;
	height:24px;
	line-height:24px;
	font-size:16px;
	text-align:center;
	margin-top:11px;
	margin-right:3px;
	-webkit-transition:all 0.3s;
	transition:all 0.3s;
}
.avalon-merlin-menu .title{
	display:block;
	overflow:hidden;
}
.avalon-merlin-menu-parent{
	display:block;
	height:46px;
	line-height:46px;
	padding:0px 5px 0px 2px;
	color:#FFFFFF;
	border-left:4px solid transparent;
	-webkit-transition:color .3s ease-in-out;
	-moz-transition:color .3s ease-in-out;
	transition:color .3s ease-in-out;
	-webkit-transition:background-color .3s ease-in-out;
	-moz-transition:background-color .3s ease-in-out;
	transition:background-color .3s ease-in-out;
}
.avalon-merlin-menu-parent:hover{
	background-color:#444444;
	color:#FFFFFF;
}
.avalon-merlin-menu-parent.on{background-color:#3C3C3C;}
.avalon-merlin-menu-parent.selected .icons{
	-webkit-transform: rotate(-90deg);
	transform: rotate(-90deg);
}
.avalon-merlin-menu-one:hover > .avalon-merlin-menu-parent{
	background-color:#3C3C3C;
	border-left:4px solid #009fd8;
}
.avalon-merlin-menu-child{
	overflow:hidden;
	transition:all .3s ease-in-out;
	-webkit-transition:all .3s ease-in-out;
	-moz-transition:all .3s ease-in-out;
}
.avalon-merlin-menu-child > .link{
	display:block;
	height:46px;
	line-height:46px;
	padding:0px 5px;
	color:#FFFFFF;
	border-left:4px solid transparent;
	-webkit-transition:color .3s ease-in-out;
	-moz-transition:color .3s ease-in-out;
	transition:color .3s ease-in-out;
	-webkit-transition:background-color .3s ease-in-out;
	-moz-transition:background-color .3s ease-in-out;
	transition:background-color .3s ease-in-out;
}
.avalon-merlin-menu-child > .link:hover{background-color:#3C3C3C;}
.avalon-merlin-menu-child .link.on{background-color:#009fd8;}
.avalon-merlin-menu-parent.indent,.avalon-merlin-menu-child.indent a{
	padding-left:24px;
}
.avalon-merlin-menu-parent.indent1,.avalon-merlin-menu-child.indent1 a{
	padding-left:48px;
}