@charset "utf-8";
/* CSS Document */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video{ word-wrap:break-word; word-break:break-all; padding:0; margin:0; list-style:none; outline:0; -webkit-tap-highlight-color:rgba(0,0,0,0); font:inherit; font-size:14px; vertical-align:baseline; font-family:'Microsoft YaHei',Helvetica,Arial,sans-serif; -webkit-overflow-scrolling: touch; overflow-scrolling: touch;}
body{overflow-x:hidden; font-size:1em;}
button,input,option,select,textarea,em{ -webkit-appearance:none; -webkit-text-size-adjust:none; outline:none; -webkit-tap-highlight-color:rgba(0,0,0,0); border-radius:0;}
input[type="button"], input[type="submit"], input[type="reset"] {-webkit-appearance: none; border-radius:0;}
button,img,input,nav,textarea,video{border:0;}
/*body,td{font-size:.9em}*/
a,a:visited{text-decoration:none;-webkit-touch-callout:none; -webkit-tap-highlight-color:rgba(0,0,0,0);}
body,html{height:100%}
.clear{ clear:both; padding:0; margin:0; line-height:0;}
.clearfix{zoom:1;}
.clearfix:after,.clearfix:before{content:"";display:block;height:0;width:0;visibility:hidden}
.clearfix:after{clear:both;}

/*.loginBG{ background: url(../images/whitebg.jpg) no-repeat scroll center bottom #FFFFFF; background-size:100%; background-attachment:fixed;}*/
.bg{ background: #f5f5f5;}

.Logo{ text-align: center; font-size: 36px; color: #8CC9F5; font-weight: bold; padding: 8rem 0 6rem;}
.Logo img{ width:150px;}
.Logo p{ font-weight: normal; font-size: 15px;}

.LoginBox{ padding: 0 20px;}
.LoginBox ul li{ padding: 10px 0;}
.LoginBox ul li input[type='text']{ height: 40px; line-height: 40px; padding: 0 10px; border-bottom:1px solid #eaeaea; width: 100%; box-sizing: border-box;}
.LoginBox ul li input[type='password']{ height: 40px; line-height: 40px; padding: 0 10px; border-bottom:1px solid #eaeaea; width: 100%; box-sizing: border-box;}
.LoginBox .bts{ margin: 20px 0 0;}
.LoginBox .bts input.bt1{ width: 100%; background:#8cc9f5; color: #FFFFFF; height:40px; font-size: 15px; line-height: 40px; border-radius: 3px;}

.mainBack{ position: fixed; left: 0; top:50px; right: 0; bottom: 0; margin:auto;}
.mainList{ width: 100%; height:100%; overflow: hidden; overflow-y: auto;}
.mainList_1{ padding: 0 0 20px;}
.choList{ padding: 0 10px;}
.choList ul li{ padding: 10px 0;}
.choList ul li em{ display: block; padding: 0 0 8px; color: #333333;}
.choList ul li .rightContent{ overflow: hidden;}
.choList ul li .rightContent p{ color: #666666;}
.choList ul li .rightContent p span{ color: #8CC9F5; font-size: 16px; padding: 0 5px;}
.choList ul li .rightContent .dateBox{ position: relative;}
.choList ul li .rightContent .dateBox span.icon{ position: absolute;  top: 8px; right: 5px;}
.choList ul li .rightContent .dateBox span.icon i{ font-size: 22px; color: #999999;}
.choList ul li .rightContent input.text{ width: 100%; background: none; height: 35px; line-height: 35px; border-bottom: 1px solid #EAEAEA;}
.choList ul li .rightContent .checkboxBts label{float: left; border: 1px solid #EAEAEA; margin: 0 10px 10px 0; height: 30px; line-height: 30px; text-align: center; position: relative; border-radius: 3px; padding: 0 15px;}
.choList ul li .rightContent .checkboxBts label input{ display: block; position: absolute; left: 0; top: 0; width: 100%; height: 100%;}
.choList ul li .rightContent .checkboxBts label.on{ background: #8CC9F5; color: #FFFFFF; border:1px solid #8CC9F5;}

.choList ul li .rightContent .radioBts label{ float: left; border: 1px solid #EAEAEA; margin: 0 10px 10px 0; height: 30px; line-height: 30px; text-align: center; position: relative; border-radius: 3px; padding: 0 15px;}
.choList ul li .rightContent .radioBts label input{ display: block; position: absolute; left: 0; top: 0; width: 100%; height: 100%;}
.choList ul li .rightContent .radioBts label.on{ background: #8CC9F5; color: #FFFFFF; border:1px solid #8CC9F5;}

.choResult{ border: 1px solid #b9d4f1; border-radius: 5px; margin: 0 10px;}
.choResult h4{ height: 40px; line-height: 40px; color: #77a4d4; font-weight: bold; padding: 0 10px;}
.choResult .resultMessage{ padding:0 10px 10px;}
.choResult .resultMessage span{ display: inline-block; padding: 0 20px; border:1px solid #b9d4f1; background: #eef5fd; height: 25px; line-height: 25px; border-radius: 3px; overflow: hidden; margin: 0 5px; vertical-align: middle;}

.xuanke{ margin:10px; height: 40px; line-height: 40px; background: #ecf7ff; text-align: center; color: #77A4D4; border-radius:50px; font-size: 16px;}
.xuanke select{ width: 100%; height: 100%; background: none; border: none; text-align: center; text-align-last: center; padding:0 20px}
.xuanke select option{ text-align: center;
}
