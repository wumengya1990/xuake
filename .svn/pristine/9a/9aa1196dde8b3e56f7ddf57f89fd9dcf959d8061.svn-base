.bottomBts{ position: absolute; left: 0; top: auto; bottom: 0; right:0; margin:auto; box-sizing: border-box; padding:10px;}
.bottomBts input{ display: block; background: #8CC9F5; border: 1px solid #8CC9F5; color: #FFFFFF; height: 40px; line-height: 40px; width: 100%; border-radius: 3px;}
.bottomBts input.on{ display: block; background: #FFFFFF; border: 1px solid #8CC9F5; color: #8CC9F5; height: 40px; line-height: 40px; width: 100%; border-radius: 3px;}


.classContent{ padding: 0 10px;}
.classContent .task{ background: #FFFFFF; margin: 10px 0 0; padding: 10px; border-radius: 5px; position: relative;}
.classContent .task h3{ font-size: 16px; color: #333333; margin:0 0 5px;}
.classContent p.state span.ready{ display: inline-block; background: #e8f3ff; height: 25px; line-height: 27px; color: #69b6ef; padding: 0 10px; border-radius: 50px; font-size: 13px;}
.classContent p.state span.end{ display: inline-block; background: #F5F5F5; height: 25px; line-height: 27px; color: #999999; padding: 0 10px; border-radius: 50px; font-size: 13px;}
.classContent .task i{ position: absolute; left:auto; top: 0; bottom: 0; right: 10px; margin:auto; height: 16px; font-size: 18px; color: #999999;}

.classContent .subject{background: #FFFFFF; margin: 10px 0 0; padding: 10px; border-radius: 5px; position: relative;}
.classContent .subject h3{ font-size: 16px; color: #333333; margin:0 0 5px;}
.classContent .subject ul li{ padding: 5px 0;}
.classContent .subject ul li em{ float: left; color: #999999; margin: 0 10px 0 0;}
.classContent .subject ul li .Rmessage{ overflow: hidden;}
.classContent .subject .bts{ position: absolute; left: auto; top: 0; right: 0; bottom: 0; margin:auto;}
.classContent .subject .bts input[type='button']{ border:1px solid #8CC9F5; background:#8CC9F5; color: #FFFFFF; height: 30px; line-height: 30px; width: 80px; text-align: center; border-radius: 3px; margin: 10px;}
.classContent .subject .bts input.on{ border:1px solid #8CC9F5; background: none; color: #8CC9F5; height: 30px; line-height: 30px; width: 80px; text-align: center; border-radius: 3px; margin: 10px;}

/*课程详情*/
.subjectDetails{ padding:10px;}
.subjectDetails h2{ margin: 0 0 5px; height: 35px; line-height: 35px;}
.subjectDetails h2 span{ font-size: 18px;}
.subjectDetails h2 a.backPage{ float: right;}
.subjectDetails h2 a.backPage i{ font-size: 24px; color: #999999;}
.subjectDetails .courseImg{ width: 100%;}
.subjectDetails .courseImg img{ width: 100%; overflow: hidden; border-radius:5px;}
.subjectDetails ul{ margin: 10px 0 0;}
.subjectDetails ul li{ padding: 5px 0; float:left; width: 100%;}
.subjectDetails ul li em{ float: left; color: #999999; margin: 0 10px 0 0;}
.subjectDetails ul li .Rmessage{ overflow: hidden;}
.subjectDetails ul li .Rmessage p{ font-size: 14px;}
.subjectDetails .courseDescribe{ border-bottom:1px solid #EAEAEA; padding:10px 0;}
.subjectDetails .courseDescribe p{ text-indent: 2em; color: #888888; line-height: 1.6em;}

/*顶部返回条*/
.backTop{ height:40px; line-height: 40px; background: #FFFFFF; border-bottom: 1px solid #eaeaea; }
.backTop span{ padding: 0 10px; font-size:16px;}
.backTop a{ float: right; height: 40px; line-height:40px; margin: 0 5px; padding: 0 10px;  font-size: 18px;}
.backTop a i{ font-size:22px; color:#999999;}

/*帐号信息*/
.userTop{ height:50px; background: #47b2ff;}
.userMes{ overflow: hidden; color: #FFFFFF;}
.userMes .portrait{ float: left; width: 36px; height: 36px; margin: 5px; border-radius:50%; overflow: hidden; box-shadow:0 1px 2px #3185c3; border:2px solid #eaeaea;}
.userMes .portrait img{ width: 100%; height: 100%;}
.userMes h4{ font-size: 15px; margin: 8px 0 0;}
.userMes p{ font-size: 12px; color: #8CC9F5;}
.userTop .changePeo{ float: right; width: 30px; height: 30px; background: #6abefb; text-align: center; line-height: 30px; color: #FFFFFF; border-radius: 50%; margin: 10px;}

